{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "includeEntryExports": true,
  "ignoreExportsUsedInFile": false,
  "bun": true,
  "expo": true,
  "postcss": true,
  "storybook": true,
  "tailwind": true,
  "playwright": true,
  "typescript": true,
  "ignore": [
    "**/generated/**",
    "types/**"
  ],
  "ignoreDependencies": [
    "happy-dom",
    "tailwindcss",
    "wrangler"
  ],
  "ignoreBinaries": ["act", "k6", "maestro"],
  "workspaces": {
    ".": {
      "entry": ["scripts/*.ts"]
    },
    "packages/core": {
      "includeEntryExports": false,
      "entry": ["src/**/*.test.ts"]
    },
    "packages/client": {
      "includeEntryExports": false
    },
    "packages/client-react": {
      "includeEntryExports": false,
      "entry": [
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/__tests__/test-utils.ts",
        "src/__tests__/integration/test-setup.ts"
      ]
    },
    "packages/client-plugin-encryption": {
      "includeEntryExports": false
    },
    "packages/console": {
      "includeEntryExports": false
    },
    "packages/server": {
      "includeEntryExports": false
    },
    "packages/server-hono": {
      "includeEntryExports": false,
      "entry": ["src/**/*.test.ts", "scripts/*.ts"]
    },
    "packages/server-cloudflare": {
      "includeEntryExports": false
    },
    "packages/server-dialect-sqlite": {
      "includeEntryExports": false
    },
    "packages/server-dialect-postgres": {
      "includeEntryExports": false
    },
    "packages/server-service-worker": {
      "includeEntryExports": false
    },
    "packages/server-storage-filesystem": {
      "includeEntryExports": false
    },
    "packages/server-storage-s3": {
      "includeEntryExports": false
    },
    "packages/transport-http": {
      "includeEntryExports": false
    },
    "packages/transport-ws": {
      "includeEntryExports": false
    },
    "packages/relay": {
      "includeEntryExports": false
    },
    "packages/migrations": {
      "includeEntryExports": false
    },
    "packages/typegen": {
      "includeEntryExports": false
    },
    "packages/testkit": {
      "includeEntryExports": false
    },
    "packages/syncular": {
      "includeEntryExports": false
    },
    "packages/observability-sentry": {
      "includeEntryExports": false
    },
    "packages/dialect-bun-sqlite": {
      "includeEntryExports": false
    },
    "packages/dialect-better-sqlite3": {
      "includeEntryExports": false
    },
    "packages/dialect-pglite": {
      "includeEntryExports": false
    },
    "packages/dialect-sqlite3": {
      "includeEntryExports": false
    },
    "packages/dialect-neon": {
      "includeEntryExports": false
    },
    "packages/dialect-libsql": {
      "includeEntryExports": false
    },
    "packages/dialect-d1": {
      "includeEntryExports": false
    },
    "packages/dialect-wa-sqlite": {
      "includeEntryExports": false
    },
    "packages/dialect-expo-sqlite": {
      "includeEntryExports": false
    },
    "packages/dialect-react-native-nitro-sqlite": {
      "includeEntryExports": false
    },
    "packages/dialect-electron-sqlite": {
      "includeEntryExports": false
    },
    "packages/ui": {
      "includeEntryExports": false
    },
    "apps/demo": {
      "includeEntryExports": false,
      "entry": [
        "src/main.tsx!",
        "src/cf/index.ts!",
        "scripts/*.ts"
      ]
    },
    "apps/console": {
      "entry": ["src/main.tsx!"]
    },
    "apps/docs": {
      "includeEntryExports": false,
      "entry": [
        "source.config.ts",
        "open-next.config.ts",
        "scripts/*.ts"
      ]
    },
    "tests/unit": {
      "entry": ["**/*.test.ts"]
    },
    "tests/dialects": {
      "includeEntryExports": false,
      "entry": ["**/*.ts"]
    },
    "tests/perf": {
      "entry": ["**/*.ts"]
    },
    "tests/load": {
      "includeEntryExports": false,
      "entry": ["lib/*.js", "scripts/*.js", "seeding.ts"],
      "ignoreDependencies": ["k6"]
    },
    "tests/integration": {
      "includeEntryExports": false,
      "entry": ["**/*.ts"]
    },
    "tests/runtime": {
      "includeEntryExports": false,
      "entry": ["**/*.ts"]
    },
    "tests/typegen": {
      "includeEntryExports": false,
      "entry": ["**/*.ts"]
    },
    "tests/expo-app": {
      "entry": ["**/*.ts", "**/*.tsx"]
    }
  }
}
